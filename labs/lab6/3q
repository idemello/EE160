#include <stdio.h>

int main()
{
  int a;
  int b;
  int c;
  int d;
  int f;
  int others;

      'a' ='b' = 'c' = 'd' = 'f' = 0;
       others = 0;
       while ((c = getchar()) != EOF)
       {  printf("%c\n", c);
              switch(c)
              {
                 case 'a':
					  case 'A':
							 a++;
                 case 'b':
					  case 'B':
							 b++;
                 case 'c':
					  case 'C':
							 c++;
                 case 'd':
				     case 'D':
							 d++;
                 case 'f':
					  case 'F':
							 f++;

                 default: others++;
              }
       }
       printf("Grade counts:\n");

       printf("  A's: %d\n", a);
       printf("  B's: %d\n", b);
       printf("  C's: %d\n", c);
       printf("  D's: %d\n", d);
       printf("  F's: %d\n", f);
       printf("  Other grades: %d\n", others);

}
